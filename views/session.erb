<a href="/device/<%= @session.device.core_id %>">Back all readings</a>

<%= line_chart @readings.map {|r| [r.published_at, r.spo2]}, library: {vAxis: {title: "SpO2", viewWindow: {max: 100, min: 90}}} %>

<%= line_chart @readings.map {|r| [r.published_at, r.hr]}, library: {curveType: "function", vAxis: {title: "Heart Rate"}} %>